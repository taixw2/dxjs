{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "generator-star-spacing": [
      0
    ],
    "consistent-return": [
      0
    ],
    "radix": [
      1
    ],
    "no-unused-vars": 2,
    "global-require": [
      0
    ],
    "import/prefer-default-export": [
      0
    ],
    "no-else-return": [
      0
    ],
    "no-restricted-syntax": [
      0
    ],
    "no-use-before-define": [
      0
    ],
    "no-nested-ternary": [
      0
    ],
    "arrow-body-style": [
      0
    ],
    "import/extensions": [
      0
    ],
    "no-bitwise": [
      0
    ],
    "no-cond-assign": [
      0
    ],
    "import/no-unresolved": [
      0
    ],
    "require-yield": [
      1
    ],
    "no-param-reassign": [
      0
    ],
    "no-shadow": [
      0
    ],
    "no-underscore-dangle": [
      0
    ],
    "spaced-comment": [
      0
    ],
    "indent": [
      0
    ],
    "quotes": [
      0
    ],
    "func-names": [
      0
    ],
    "arrow-parens": [
      0
    ],
    "space-before-function-paren": [
      0
    ],
    "no-useless-escape": [
      0
    ],
    "object-curly-newline": [
      0
    ],
    "function-paren-newline": [
      0
    ],
    "class-methods-use-this": [
      0
    ],
    "no-new": [
      0
    ],
    "no-console": [
      1
    ]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
